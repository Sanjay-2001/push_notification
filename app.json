{
  "expo": {
    "name": "Push Notification",
    "slug": "push_notification",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/notification-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile" :"./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/notification-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "dev.expo.notificationsapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/notification-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/notification-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "916e2576-f208-46e5-814e-eb3906170873"
      }
    }
  }
}
